{
  "kind": "collectionType",
  "collectionName": "deportes",
  "info": {
    "singularName": "deporte",
    "pluralName": "deportes",
    "displayName": "Deporte"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Image": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "noticias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::noticia.noticia",
      "mappedBy": "deporte"
    }
  }
}
